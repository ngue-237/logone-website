    </div>
    <div class="row feature_inner">
      {% for catg in categoriesService %}
      <div class="col-lg-3 col-md-6">
        <a href="{{ path('services', { slug: catg.slug }) }}" class="text-font">
          <div class="feature_item">
            <img
              src="{{ asset('/uploads/images/categories_service/' ~ catg.image) }}"
              alt="image parlant de {{ catg.designation }}"
            />
            <h4>{{ catg.designation }}</h4>
            <p>{{ catg.description | raw }}</p>
          </div>
        </a>
      </div>
      {% endfor %}
      <div class="pagination text-center">
        {{ knp_pagination_render(categoriesService) }}
      </div>
    </div>
  </div>
</section>

