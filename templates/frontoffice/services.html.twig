{% extends 'baseF.html.twig'%} {% block title %} {{ categories.slug }} | Logone
{% endblock %} {% block meta %}
{{ sonata_seo_metadatas("description") }}
<meta
  name="keywords"
  content="
  {% for service in services %}
  {{ service.slug }},
  {% endfor %}
  "
/>
{% endblock %} {% block content %}

<!--================ Start Features Area =================-->
<section class="features_area section_gap_top" style="margin-bottom: 80px">
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-lg-8 text-center">
        <div class="main_title">
          <h2>Nos services</h2>
          <p>
            {{ categories.description | raw }}
          </p>
        </div>
        <div class="text-center">
          <a
            style="position: relative; top: -55px"
            href="{{ path('devis_add', { slug: categories.slug }) }}"
            class="genric-btn success circle"
          >
            <span>Je suis intéressé</span>
          </a>
        </div>
      </div>
    </div>
    <div class="row feature_inner">
      {% set i= 0 %} {% for service in services%} {% set i= i+1 %}
      <div class="accordion col-lg-8 mt-2" id="accordionExample">
        <div class="accordion-item">
          <h2 class="accordion-header" id="headingThree">
            <button
              class="accordion-button collapsed"
              type="button"
              data-bs-toggle="collapse"
              data-bs-target="#collapseThree{{ i }}"
              aria-expanded="false"
              aria-controls="collapseThree"
            >
              {{ service.designation }}
            </button>
          </h2>
          <div
            id="collapseThree{{ i }}"
            class="accordion-collapse collapse"
            aria-labelledby="headingThree"
            data-bs-parent="#accordionExample"
          >
            <div class="accordion-body">
              {{ service.description | raw }}
            </div>
          </div>
        </div>
      </div>
      {% endfor %}
      <div class="col-lg-4">
        <div class="text-center mt-20">
          <img
            src="{{ asset('/uploads/images/categories_service/' ~ categories.image) }}"
            class="rounded"
            alt="images"
          />
        </div>
      </div>
    </div>
  </div>
</section>
<!--================ End Features Area =================-->
