{% extends 'baseF.html.twig'%} {% block title %} Categories-Services | Logone {%
endblock %} {% block meta %}
<meta name="description" content="" />
<meta
  name="keywords"
  content="MARKETING DIGITAL, RELATION CLIENT, CENTRE D'APPEL, EXTERNALISATION-CENTRE-APPEL-TUNISIE"
/>
{% endblock %} {% block content %}

<!--================ Start Features Area =================-->
<section class="features_area section_gap_top">
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-lg-8 text-center">
        <div class="main_title">
          <h2>Nos services</h2>
          <p class="sample-text">
            Nous sommes une agence de marketing et de communication experte dans
            les principaux canaux de
            <span style="color: #4cd3e3; font-weight: bold"
              >communication digitale</span
            >
            et de la
            <span style="color: #4cd3e3; font-weight: bold">
              gestion de la relation client </span
            >. <br />
            De la création de votre site internet en passant par la mise en
            passant d’une stratégie digitale pour augmenter votre visibilité,
            ventes et notoriété sur le web, nous vous accompagnons dans le
            développement de votre business.
          </p>
        </div>
      </div>
    </div>
    <div class="row feature_inner">
      {% for catg in catgs %}
      <div class="col-lg-3 col-md-6">
        <a class="text-font" title="lien vers le service : {{ catg.slug }} ">
          <div class="feature_item">
            <h4>
              {{ catg.designation }}
            </h4>
            <p>
              {{ catg.description | striptags | slice(0,200)  | raw  }}
              ...
            </p>

            <a
              style="margin-top: 20px"
              title="lien nos services de la catégorie : {{ catg.slug }}"
              href="{{ absolute_url(path('services', { slug: catg.slug })) }}"
              class="genric-btn success circle"
            >
              <span>Voir plus</span>
            </a>
          </div>
        </a>
      </div>
      {% endfor %}
      <div
        class="blog-pagination justify-content-center d-flex"
        style="position: relative; top: -20px"
      >
        {{ knp_pagination_render(catgs) }}
      </div>
    </div>
  </div>
</section>

